{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.tabSize": 2,
    "[vue]": {
        "editor.suggest.insertMode": "insert"
    },
    "tailwindCSS.experimental.configFile": ".nuxt/tailwind.config.cjs",
    "conventionalCommits.scopes": [
        "website",
        "apps"
    ]
}